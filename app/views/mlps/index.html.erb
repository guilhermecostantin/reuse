<h1>Listing mlps</h1>

<table>
  <tr>
    <th>Entrada1</th>
    <th>Entrada2</th>
    <th>Entrada3</th>
    <th>Entrada4</th>
    <th>Target1</th>
    <th>Target2</th>
    <th>Target3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mlps.each do |mlp| %>
  <tr>
    <td><%= mlp.entrada1 %></td>
    <td><%= mlp.entrada2 %></td>
    <td><%= mlp.entrada3 %></td>
    <td><%= mlp.entrada4 %></td>
    <td><%= mlp.target1 %></td>
    <td><%= mlp.target2 %></td>
    <td><%= mlp.target3 %></td>
    <td><%= link_to 'Show', mlp %></td>
    <td><%= link_to 'Edit', edit_mlp_path(mlp) %></td>
    <td><%= link_to 'Destroy', mlp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

